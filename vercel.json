{
  "rewrites": [
    {
      "source": "/",
      "destination": "https://legacy.hakadori-soudan.com/",
      "has": [
        {
          "type": "query",
          "key": "p"
        }
      ]
    },
    {
      "source": "/office/(.*)",
      "destination": "/office/$1"
    },
    {
      "source": "/?p=:p",
      "destination": "https://legacy.hakadori-soudan.com/?p=:p"
    },
    {
      "source": "/(.*)",
      "destination": "https://legacy.hakadori-soudan.com/$1"
    }
  ]
}
